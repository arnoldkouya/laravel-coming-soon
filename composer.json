{
    "name": "arnoldkouya/lara-coming-soon",
    "description": "A Laravel package for managing maintenance modes with IP whitelisting",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnold Kouya",
            "email": "arnoldkouya@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ArnoldKouya\\LaraComingSoon\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0",
        "illuminate/http": "^8.0 || ^9.0 || ^10.0",
        "illuminate/database": "^8.0 || ^9.0 || ^10.0",
        "illuminate/routing": "^8.0 || ^9.0 || ^10.0"
    },
    "conflict": {
        "illuminate/support": "<8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArnoldKouya\\LaraComingSoon\\LaraComingSoonServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "replace": {
        "illuminate/support": "*"
    }
}
